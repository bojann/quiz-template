  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/hookrouter": "^2.2.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.14",
    "axios": "^0.21.4",
    "eslint-config-prettier": "^6.11.0",
